#include "mainwindow.h"
#include <QApplication>
#include<QPushButton>
#include<QScrollBar>
#include<QSlider>
int main(int argc, char *argv[])
{
    QApplication prog(argc, argv);
   QPushButton* button = new QPushButton("Close");
   QObject::connect(button,SIGNAL(clicked()),&prog,SLOT(quit()));
   button->show();
    return prog.exec();

}
